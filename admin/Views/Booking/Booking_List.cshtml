@{
    ViewBag.Title = "Booking_List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!-- Content Header (Page header) -->
<section class="content-header">
    <h1 class="fa fa-plane">
        Quản lý vé
        @*<small>advanced tables</small>*@
    </h1>
    <ol class="breadcrumb">
        <li><a href="/"><i class="fa fa-home"></i> Trang chủ</a></li>
        <li><a href="#">Quản lý vé</a></li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <!-- SELECT2 EXAMPLE -->
    <div class="box box-default">
        <div class="box-header with-border">
            <h3 class="box-title fa fa-search"> Tìm kiếm</h3>

            <div class="box-tools pull-right">
                <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                @*<button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-remove"></i></button>*@
            </div>
        </div>
        <!-- /.box-header -->
        <div class="box-body">
            <div class="row">
                <div class="col-md-12">
                    <div class="col-md-4">
                        <div class="form-group">
                            <label>Chuỗi tìm kiếm</label>
                            <input type="text" class="form-control" id="" placeholder="Nhập chuỗi tìm kiếm...">
                        </div>
                        <!-- /.form-group -->
                        <div class="form-group">
                            <div class="row">
                                <div class="col-md-6">
                                    <label>Trạng thái vé</label>
                                    <select class="form-control">
                                        <option>Tất cả</option>
                                        <option>Đang giữ chỗ</option>
                                        <option>Đã xuất vé</option>
                                        <option>Đã hoàn vé</option>
                                        <option>Thay đổi hành trình</option>
                                        <option>Chuyến bay hoàn tất</option>
                                        <option>Vé sắp bay</option>
                                    </select>
                                </div>
                                <div class="col-md-6">
                                    <label>Tình trạng thanh toán</label>
                                    <select class="form-control">
                                        <option>Tất cả</option>
                                        <option>Chưa thanh toán</option>
                                        <option>Đã thanh toán</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <!-- /.form-group -->
                    </div>
                    <!-- /.col -->
                    <div class="col-md-2">
                        <div class="form-group">
                            <label>Đặt chỗ từ ngày:</label>

                            <div class="input-group date">
                                <div class="input-group-addon">
                                    <i class="fa fa-calendar"></i>
                                </div>
                                <input type="text" class="form-control pull-right" id="datepicker">
                            </div>
                            <!-- /.input group -->
                        </div>
                        <!-- /.form-group -->
                        <div class="form-group">
                            <label>Đặt chỗ đến ngày:</label>

                            <div class="input-group date">
                                <div class="input-group-addon">
                                    <i class="fa fa-calendar"></i>
                                </div>
                                <input type="text" class="form-control pull-right" id="datepicker">
                            </div>
                            <!-- /.input group -->
                        </div>
                        <!-- /.form-group -->
                    </div>
                    <!-- /.col -->
                    <div class="col-md-2">
                        <div class="form-group">
                            <label>Ngày bay:</label>

                            <div class="input-group date">
                                <div class="input-group-addon">
                                    <i class="fa fa-calendar"></i>
                                </div>
                                <input type="text" class="form-control pull-right" id="datepicker">
                            </div>
                            <!-- /.input group -->
                        </div>
                        <!-- /.form-group -->
                        <div class="form-group">
                            <label>Người xuất vé</label>
                            <select class="form-control select2" style="width: 100%;">
                                <option selected="selected">Tất cả</option>
                                <option>Alaska</option>
                                <option>California</option>
                                <option>Delaware</option>
                                <option>Tennessee</option>
                                <option>Texas</option>
                                <option>Washington</option>
                            </select>
                        </div>
                        <!-- /.form-group -->
                    </div>
                    <div class="col-md-4">
                        <div class="form-group">
